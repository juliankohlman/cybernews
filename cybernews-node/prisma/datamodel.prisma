# redeploy commands (apply changes and migrate underlying db schema)
# prisma deploy
# primsa generate => updates prisma client with CRUD op's for newly added models
type Link {
	id: ID! @unique
	description: String!
	url: String!
	postedBy: User
}

type User {
	id: ID! @unique
	name: String!
	email: String! @unique
	password: String!
	links: [Link!]!
}
